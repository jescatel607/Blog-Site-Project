<div class="author">
  <h1 class="author__heading">Search Results</h1>
</div>

<section class="articles">
  <h2 class="articles__heading">Search Results</h2>

  <!-- Dummy data displayed until we include posts later on -->
  <ul class="article_ul">
    <% data.forEach(post => { %>
    <li>
      <a href="/post/<%= post._id %>">
        <span><%- post.title %></span>
        <span class="article-list_date">
          <%= post.createdAt.toDateString() %>
        </span>
      </a>
    </li>
    <%}); %>
  </ul>
</section>
